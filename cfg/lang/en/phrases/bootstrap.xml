<?xml version="1.0" encoding="utf-8" standalone="no"?>
<!DOCTYPE phrases SYSTEM "entities.dtd">
<epp:phrases xmlns="http://www.w3.org/1999/xhtml" xmlns:epp="http://eprints.org/ep3/phrase" xmlns:epc="http://eprints.org/ep3/control">

	<epp:phrase id="bootstrap/branding_header:css">
/**********
  Top Bit   
**********/

#bs_navbar {
 border: none;
 border-radius: 0;
 min-height: 4em;
 margin-bottom: 0!important;
 background: #0094d5;
}

#bs_navbar a.navbar-brand, #bs_navbar a {
 color: #fff!important;
}
#bs_navbar, #uel-navbackground a {
 color: #0094d5; //think this is for the mobile view
}

#bs_navbar .navbar-collapse {
 border: none;
 box-shadow: none;
}

#bs_navbar .navbar-header .brand {
}

#bs_navbar .navbar-default .navbar-toggle:hover,#bs_navbar .navbar-default .navbar-toggle:focus {
 background-color: #fff;
}
#bs_navbar .navbar-default .navbar-toggle .icon-bar {
 background-color: #fff;
}
#bs_navbar .navbar-default .navbar-toggle:focus .icon-bar , #bs_navbar .navbar-default .navbar-toggle:hover .icon-bar {
 background-color: #009bd9;
}

#topnav > ul > li.active {
 display: none;
}

@media screen and (min-width:768px){
#bs_navbar {
 font-size: 10px;
 font-family: Arial;
}

#bs_navbar, #bs_navbar .navbar-header {
 height: 4em;
}

#bs_navbar .navbar a, #bs_navbar a.navbar-brand {
text-decoration: none;
-webkit-transition: all 0.15s ease-in-out;
-moz-transition: all 0.15s ease-in-out;
-ms-transition: all 0.15s ease-in-out;
-o-transition: all 0.15s ease-in-out;
transition: all 0.15s ease-in-out;
//line-height: 2.5em;
color: #fefefe;
font-family: Arial;
display: inline-block;
height: 100%;
text-align: center;
//background: #0094d5;
}

.navbar-header a, #topnav a {
font-size: 16px;
}

#bs_navbar > ul > li.active > a, #bs_navbar > ul > li:hover > a, .navbar-header a:hover, .social-icons-container li a:hover {
background: #003471;
color: white;
}

#bs_navbar .navbar-left a:hover, #bs_navbar .navbar-header a:hover {
background: #003471!important;
}
.social-icons-container li i:hover {
background-color: #003471!important;
}

/*#bs_navbar .navbar-brand, #bs_navbar > .navbar-nav > li > a, #bs_navbar .navbar-brand, #bs_navbar > .navbar-nav > li > span {*/
#bs_navbar .navbar-header a, #bs_navbar .navbar-left a {
padding-top: 10px!important;
padding-bottom: 10px!important;
}

}

/************
 Social Icons
************/

.social-icons-container ul {
list-style: none;
padding: 0;
margin: 0;
}
.social-icons-container {
width: 193px;
margin-left: 5px;
float: right;
position: relative;
}
.social-icons-container li a {
padding: 0px!important;
width: 100%!important
}
.social-icons-container li {
width: 25%;
position: relative;
height: 2.5em;
float: left;
display: inline-block;
}

.uelicon {
background: url('http://roartest.uel.ac.uk/roar_template/Images/uelicons.png') no-repeat;
width: 48px;
height: 39px;
display: inline-block;
}

.icon-fb {
background-position: 10px -172px;
}
.icon-yt {
background-position: 10px -1210px;
}
.icon-tw {
background-position: 10px -973px;
}
.icon-gp {
background-position: 10px -409px;
}
	</epp:phrase>

